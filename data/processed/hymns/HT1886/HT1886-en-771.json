  "verses": [
    {
      "number": 1,
      "text": "Jesus, Lover of my soul,\nWhile the billows near me roll,\nWhile the tempest still is high!\nO receive my soul at last!"
    },
    {
      "number": 2,
      "text": "Hide me, O my Saviour, hide!\nTill the storm of life is past;"
    }
  ],
